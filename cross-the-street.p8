pico-8 cartridge // http://www.pico-8.com
version 42
__lua__
function _init()
	t=0
	sprites={2,3}
	dance_offset=10
	x=7
	y=0
end

function _update()
	t+=1
	sprite = sprites[(t\dance_offset) % 2 + 1]

	if btnp(0) then x-=1 end
 if btnp(1) then x+=1 end
 if btnp(2) then y-=1 end
 if btnp(3) then y+=1 end
 
 if y>14 then y=14 end
 if x>15 then x=15 end
 if x<0 then x=0 end
 if y<0 then y=0 end
end

function _draw()
	cls()
	map()
	print(x .. ", " .. y)
	spr(sprite, x*8, y*8,1,2)
	spr(128,14*8,4*8,2,1)
end
	
