pico-8 cartridge // http://www.pico-8.com
version 42
__lua__

#include lib/dance_sprite.lua

function _init()
   t=0
   turn=1
   sprites={2,3}
   x=7
   y=0
   debug=''
   
   music"0"
end

function _update()
   t+=1

   move_player(btnp())
end

function _draw()
   cls()
   map()
   
   --print(x .. ", " .. y)
   print("turn " .. turn)
   print("btn: " .. int_to_bin_string(btn()) .. "," .. btn())
   print(debug)
   dance_sprite(sprites, x*8, y*8,1,2)
   spr(128,14*8,4*8,2,1)
end


function move_player(btn_code)
   --temp: stuff with buttons
   if(16 == btn_code) then --z
      debug=int_to_bin_string( peek(0x3101))
   end
   if(32 == btn_code) then --x
      poke(0x3101,194)
   end
   
   if((btn_code & 0b1111) != 0) then
      local mx, my = x, y
      
      if btnp(0) then mx-=1 end
      if btnp(1) then mx+=1 end
      if btnp(2) then my-=1 end
      if btnp(3) then my+=1 end

      if(my<15 and my>-1 and mx<16 and mx>-1) then
         run_turn()
         x,y=mx,my
      end
   end
end


function run_turn()
   turn+=1
   play_move_sound()
end


function play_move_sound()
   if(stat(46) == -1) then
      sfx(0)
   end
end


function int_to_bin_string(num)
   local bin=""
   for i=0,7 do
      bin=band(num,2^i)\2^i..bin
   end

   return bin
end


